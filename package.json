{
  "name": "tour-of-heroes",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "ng serve",
    "startPublic": "ng serve --host=0.0.0.0",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor ./protractor.conf.js",
    "e2eRemote": "protractor --baseUrl='http://<your-public-webspace>/'",
    "e2eSauceLabs": "echo 'INFO: make sure (a) your set the appropriate public url in protractor.conf.sauce-labs.js and (b) set the ENV variables SAUCE_USERNAME and SAUCE_ACCESS_KEY' && tsc --project e2e/ && protractor ./protractor.conf.sauce-labs.js",
    "e2eSauceLabsConnect": "echo 'INFO: make sure you are (a) running your test system locally, (b) run SauceLabsConnect and (c) set the ENV variables SAUCE_USERNAME and SAUCE_ACCESS_KEY' && tsc --project e2e/ && protractor ./protractor.conf.sauce-labs-connect.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "4.2.0",
    "@angular/compiler": "4.2.0",
    "@angular/core": "4.2.0",
    "@angular/forms": "4.2.0",
    "@angular/http": "4.2.0",
    "@angular/platform-browser": "4.2.0",
    "@angular/platform-browser-dynamic": "4.2.0",
    "@angular/router": "4.2.0",
    "core-js": "2.4.1",
    "rxjs": "5.1.0",
    "ts-helpers": "1.1.1",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.2.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.60",
    "@angular/cli": "1.1.1",
    "angular-in-memory-web-api": "0.2.4",
    "codelyzer": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "protractor": "4.0.13",
    "ts-node": "1.2.1",
    "tslint": "4.0.2",
    "typescript": "2.2.0",
    "webpack": "2.2.1"
  }
}
